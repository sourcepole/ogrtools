  END Kantonsgrenzen;


  TOPIC Landesgrenzen =

    CLASS Landesgrenzabschnitt =
      ATTRIBUTE
        Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
        Gueltigkeit: MANDATORY (
          rechtskraeftig,
          streitig,
          provisorisch,
